{
  "exclude": ["build", "node_modules", "**/*.test.ts"],
  "compilerOptions": {
    "target": "es2015",
    "module": "Node16",
    "moduleResolution": "Node16",
    "outDir": "./build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@hypertune/openfeature-base-provider": [ "../base-provider/src" ]
    }
  }
}
